<h2>Welcome to the profile of <%= @user.username.titleize %></h2>
<h2>Towns:</h2>
<b>Count: <i><%= @user.towns.count %></i></b> <br>
<% @towns.each do |town| %>
    <%= link_to town.title, town_path(town.id) %> <br>
<% end %>
<%= will_paginate @towns %>
<% if current_user == @user %>
  Incoming: <br>
  <% @incoming.each do |inc| %>
      From: <%= link_to inc.from_town, town_path(inc.from_town) %> To: <%= link_to inc.to_town, town_path(inc.to_town) %>
      Type: <%= if inc.move_type == false then "Support" else "Attack" end %>
      <div class="countdown" id=<%= inc.done_at %>></div>
  <% end %>

  Outgoing: <br>
  <% @outgoing.each do |outg| %>
      From: <%= link_to outg.from_town, town_path(outg.from_town) %> To: <%= link_to outg.to_town, town_path(outg.to_town) %>
      Type: <%= if outg.move_type == false then "Support" else "Attack" end %>
      <div class="countdown" id=<%= outg.done_at %>></div>
  <% end %>
<% end %>